# https://www.interviewbit.com/blog/self-coloring-problem/
def isSafeToColor(self, color):
    for i in range(V):
        for j in range(i + 1, V):
            if self[i][j] == 1 and color[j] == color[i]:
                return False
    return True
 
 
def printColorArray(color):
    print("Solution colors are: ")
    for i in range(len(color)):
        print(color[i], end=" ")
 
 
def graphColoring(self, m, i, color):
    if i == V:
        if isSafeToColor(self, color):
            printColorArray(color)
            return True
        return False
 
    for j in range(1, m + 1):
        color[i] = j
        if graphColoring(self, m, i + 1, color):
            return True
 
        color[i] = 0
 
    return false
